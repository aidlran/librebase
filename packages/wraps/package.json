{
  "name": "@astrobase/wraps",
  "version": "0.1.0",
  "description": "Astrobase wrap protocol implementation.",
  "keywords": [
    "astrobase",
    "cryptography"
  ],
  "homepage": "https://astrobase.me",
  "bugs": {
    "url": "https://github.com/AstrobaseTech/Astrobase/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AstrobaseTech/Astrobase.git",
    "directory": "packages/wraps"
  },
  "license": "Apache-2.0",
  "author": "Aidan Loughran",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/wraps.d.ts",
      "default": "./dist/wraps.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    },
    "./module": {
      "types": "./dist/ecdsa.d.ts",
      "default": "./dist/ecdsa.js"
    },
    "./recommended": {
      "types": "./dist/recommended.d.ts",
      "default": "./dist/recommended.js"
    }
  },
  "main": "dist/wraps.js",
  "module": "dist/wraps.js",
  "types": "./dist/wraps.d.ts",
  "files": [
    "./dist"
  ],
  "peerDependencies": {
    "@astrobase/codec-json": "~0.1.0",
    "@astrobase/core": "~0.1.0",
    "@astrobase/immutable": "~0.1.0",
    "@noble/secp256k1": "^1.7.1",
    "valibot": "~0.30.0"
