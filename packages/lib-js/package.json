{
  "name": "trusync",
  "version": "1.0.0-preview.1",
  "description": "JavaScript truSync implementation.",
  "license": "Apache-2.0",
  "private": true,
  "author": "Aidan Loughran",
  "repository": "github:aidlran/trusync",
  "bugs": "https://github.com/aidlran/trusync/issues",
  "homepage": "https://github.com/aidlran/trusync/tree/main/packages/lib-js#readme",
  "main": "dist/src/index.js",
  "bin": "dist/bin/trusync.js",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "default": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./channel": {
      "default": "./dist/src/data/channel/index.js",
      "types": "./dist/src/data/channel/index.d.ts"
    },
    "./crypto": {
      "default": "./dist/src/crypto/index.js",
      "types": "./dist/src/crypto/index.d.ts"
    },
    "./identity": {
      "default": "./dist/src/identity/index.js",
      "types": "./dist/src/identity/index.d.ts"
    },
    "./session": {
      "default": "./dist/src/session/session.js",
      "types": "./dist/src/session/session.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "lc": "echo truSync JS: $(find bin src -type f ! -name *.test.* -exec cat {} \\; | wc -l) lines",
    "start": "tsc --watch --preserveWatchOutput",
    "test": "vitest run"
  },
  "dependencies": {
    "base-x": "4.0.0",
    "tweetnacl": "1.0.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/web-worker": "^0.34.6",
    "fake-indexeddb": "^5.0.1",
    "happy-dom": "^12.10.3",
    "typescript": "~5.3.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
