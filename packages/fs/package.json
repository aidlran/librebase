      "default": "./dist/json/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "default": "./dist/middleware/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "content-type": "1.0.5"
  },
  "devDependencies": {
    "@types/content-type": "^1.1.8"
  },
  "peerDependencies": {
    "@astrobase/core": "~0.1.0"
