{
  "name": "@astrobase/immutable",
  "version": "0.1.0",
  "description": "A content addressable file system protocol with an extensible codec system.",
  "keywords": [
    "astrobase"
  ],
  "homepage": "https://astrobase.me",
  "bugs": {
    "url": "https://github.com/AstrobaseTech/Astrobase/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AstrobaseTech/Astrobase.git",
    "directory": "packages/fs"
  },
  "license": "Apache-2.0",
  "author": "Aidan Loughran",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
